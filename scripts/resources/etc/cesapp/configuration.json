{
  "container": {
    "type": "docker",
    "endpoint": "unix:///var/run/docker.sock"
  },
  "logging": {
    "type": "syslog"
  },
  "network": {
    "name": "cesnet1"
  },
  "volumes": {
    "basedir": "/var/lib/ces",
    "driver": "btrfs"
  },
  "registry": {
    "type": "etcd",
    "endpoints": [
      "http://localhost:4001"
    ]
  },
  "remote": {
    "endpoint": "https://dogu.cloudogu.com/api/v1/"
  },
  "init": {
    "basedir": "/etc/init",
    "type": "upstart"
  }
}
